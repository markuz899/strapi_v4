{
  "kind": "collectionType",
  "collectionName": "vehicles",
  "info": {
    "singularName": "vehicle",
    "pluralName": "vehicles",
    "displayName": "Vehicle",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "float",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "store": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::store.store",
      "inversedBy": "vehicles"
    },
    "model": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "vehicles"
    },
    "make": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::make.make",
      "inversedBy": "vehicles"
    },
    "type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::type.type",
      "inversedBy": "vehicles"
    },
    "quantity": {
      "type": "integer",
      "default": 1
    },
    "engine": {
      "type": "string",
      "default": "1.0",
      "required": true
    },
    "power": {
      "type": "string",
      "required": true
    },
    "doors": {
      "type": "integer",
      "default": 5,
      "required": true
    },
    "seats": {
      "type": "integer",
      "required": true,
      "default": 5
    },
    "transmission": {
      "type": "string",
      "default": "Manuale",
      "required": true
    },
    "width": {
      "type": "string"
    },
    "height": {
      "type": "string"
    },
    "long": {
      "type": "string"
    },
    "services": {
      "type": "json"
    },
    "equipment": {
      "type": "string",
      "required": true
    },
    "optionals": {
      "type": "string"
    },
    "promptDelivery": {
      "type": "boolean",
      "default": false
    },
    "superOffert": {
      "type": "boolean",
      "default": false
    },
    "powerSupply": {
      "type": "string",
      "default": "Benzina",
      "required": true
    },
    "battery": {
      "type": "string"
    },
    "luggageRack": {
      "type": "string"
    },
    "weight": {
      "type": "integer"
    },
    "optionsDeal": {
      "type": "json"
    },
    "deals": {
      "type": "json"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "visited": {
      "type": "integer",
      "default": 0
    },
    "vendor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vendor.vendor",
      "inversedBy": "vehicles"
    }
  }
}
